{
  "manifest_version": 3,
  "name": "Chrome Extension Starter",
  "description": "A Chrome Extension starter kit",
  "version": "1.0.0",
  "action": {
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/backgroundPage.js"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.myvisit.com/*"
      ],
      "js": [
        "js/contentScript.js"
      ]
    }
  ],
  "host_permissions": [
    "*://*.myvisit.com/*"
  ],
  "permissions": [
    "storage",
    "cookies",
    "webNavigation",
    "webRequest",
    "storage"
  ]
}

